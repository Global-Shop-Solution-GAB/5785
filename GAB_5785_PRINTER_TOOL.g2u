Program.Sub.gsgcFlexSched_RowCellClick.Start
v.Local.sPart.Declare
v.Local.sSchedule.Declare
v.Local.sparams.Declare
v.Local.iRefID.Declare
v.Local.sLinkID.Declare
v.Local.iLinkID.Declare
V.Local.sTemp.Declare
v.Local.iPID.Declare
v.Local.sSQL.Declare
v.Local.i.Declare
v.Local.iJob.Declare

F.Intrinsic.Control.SelectCase(v.Args.Column)
	F.Intrinsic.Control.CaseAny("PART","Part")
		v.Local.sPart.Set(v.Args.cellvalue)
		F.Intrinsic.String.Build("{0}!*!{1}!*!M",v.Local.sPart,"",V.Local.sParams)
		F.Global.General.CallWrapperSync(300011,V.Local.sParams)
		gui.frmPrint..SetFocus
		
	f.Intrinsic.Control.caseany("DOCUMENTS","Documents")
		Gui.frmPrint.gsgcFlexSched.GetCellValueByColumnName("gvFlex","Part",V.Args.RowIndex,V.Local.sPart)
		f.Global.DocumentControl.GetLinkID(v.Local.sPart.Trim,30,v.Local.iRefID)
		f.Intrinsic.Control.If(v.Local.irefid,=,-1)
			F.ODBC.Connection!con.ExecuteAndReturn("SELECT MAX(LINK_ID) AS 'LinkID' FROM LINK_DATA",v.Local.sLinkID)
			f.Intrinsic.Math.ConvertToLong(v.Local.sLinkID,v.Local.iLinkID)
			f.Intrinsic.Math.Add(v.Local.iLinkID,1,v.Local.iLinkID)
			f.Intrinsic.string.LPad(v.Local.iLinkID,"0",9,v.Local.sLinkID)
			V.Local.iRefID.Set(v.Local.iLinkID)
			F.Global.DocumentControl.CreateReference(V.Local.sPart,"00030",V.Local.iRefID)
		f.Intrinsic.Control.EndIf
		F.Global.DocumentControl.Invoke(V.Local.sPart,30,"(Inventory Master)",V.Local.iPID)
		
	f.Intrinsic.Control.caseany("PRINTDOCS","PrintDocs")
		Gui.frmPrint.gsgcFlexSched.GetCellValueByColumnName("gvFlex","Schedule",V.Args.RowIndex,V.Local.sSchedule)
		f.Intrinsic.String.Build("SELECT DISTINCT JOB, SUFFIX FROM V_JOB_CT_FLX_SCHD WHERE SCHEDULE='{0}'",V.LOCAL.sSchedule,V.Local.sSQL)
		f.Intrinsic.Control.If(v.DataTable.dtFJob.Exists)
			f.Data.DataTable.Close("dtFJob")
		f.Intrinsic.Control.EndIf
		F.Data.DataTable.CreateFromSQL("dtFJob","con",v.Local.ssql)
		f.Intrinsic.Control.For(v.Local.iJob,0,v.DataTable.dtFJob.RowCount--,1)
			f.Intrinsic.String.Build("SELECT PART FROM V_JOB_HEADER WHERE JOB='{0}' AND SUFFIX='{1}'",v.DataTable.dtFJOB(v.Local.iJob).JOB!FIELDVALTRIM,v.DataTable.dtFJob(v.Local.iJob).SUFFIX!FIELDVALTRIM,v.Local.ssql)
			f.ODBC.Connection!con.executeandreturn(v.Local.sSQL,v.Local.sPart)
			f.Global.DocumentControl.GetLinkID(v.Local.sPart.Trim,30,v.Local.iLinkID)
			f.Intrinsic.Control.If(v.Local.irefid,<>,-1)
				F.Intrinsic.String.Concat("SELECT * FROM ATG_DOC_ASSOC WHERE LINK_ID = ",V.Local.iLinkID," AND GRP_ID = 3",V.Local.sSQL)
				f.Intrinsic.Control.If(v.DataTable.dtDocs.Exists)
					f.Data.DataTable.Close("dtDocs")
				f.Intrinsic.Control.EndIf
				f.Data.DataTable.CreateFromSQL("dtDocs","con",v.Local.ssql)
				F.Intrinsic.Control.If(V.DataTable.dtDocs.RowCount,=,0)
					F.Intrinsic.UI.Msgbox("No Shop Documents to Print.")
				f.Intrinsic.Control.Else
					f.Intrinsic.Control.For(v.Local.i,0,v.DataTable.dtDocs.RowCount--,1)
						F.Intrinsic.String.Right(v.DataTable.dtDocs(v.Local.i).PATH!fieldvaltrim,1,V.Local.stemp)
						F.Intrinsic.Control.If(V.Local.stemp,=,"\")
							F.Intrinsic.String.Concat(v.DataTable.dtDocs(v.Local.i).PATH!fieldvaltrim,v.DataTable.dtdocs(v.Local.i).FILE!fieldvaltrim,V.Local.stemp)
						F.Intrinsic.Control.Else
							F.Intrinsic.String.Concat(v.DataTable.dtDocs(v.Local.i).PATH!FIELDVALTRIM,"\",v.DataTable.dtdocs(v.Local.i).FILE!fieldvaltrim,V.Local.stemp)
						F.Intrinsic.Control.endif												
						F.Intrinsic.task.ShellExec(V.Caller.Handle,"PRINT",V.local.sTemp,"","",5)
						f.Intrinsic.UI.SleepMS(1000)
					f.Intrinsic.Control.Next(v.Local.i)
				F.Intrinsic.Control.EndIf
			f.Intrinsic.Control.EndIf	
		f.Intrinsic.Control.Next(v.Local.iJob)
		
	f.Intrinsic.Control.caseany("PRINTFLEX","PrintFlex")
		Gui.frmPrint.gsgcFlexSched.GetCellValueByColumnName("gvFlex","Schedule",V.Args.RowIndex,V.Local.sSchedule)
		f.Intrinsic.String.ConcatCallWrapperArgs("01/01/1900","01/01/1900",v.Local.sSchedule,"000",v.Local.sSchedule,"000","Y","Y",v.Local.sparams)
		f.Global.General.CallWrapperAsync(900300,v.Local.sparams)
		
f.Intrinsic.Control.EndSelect
Program.Sub.gsgcFlexSched_RowCellClick.End

Program.Sub.gsgcFlexcover_RowCellClick.Start
v.Local.sparams.Declare
v.Local.sVals.Declare
v.Local.lBIID.Declare
v.Local.sJob.Declare
v.Local.sWC.Declare

F.Intrinsic.Control.SelectCase(v.Args.Column)
	f.Intrinsic.Control.caseany("PRINTCOVER","PrintCover")
		'Grab JOB & WC
		Gui.frmPrint.gsgcFlexcover.GetCellValueByColumnName("gvFlexCover","Job",V.Args.RowIndex,V.Local.sJob)
		Gui.frmPrint.gsgcFlexcover.GetCellValueByColumnName("gvFlexCover","WC",V.Args.RowIndex,V.Local.sWC)
		'Get report's ID #
		F.Global.BI.GetIDFromName("RS-SF_Flex_Cover.rpt",V.Local.lBIID)
		'add params with appropriate values
		f.Intrinsic.String.Concat("WORKORDER_WG - JOB*!*WORKORDER_WG - PART","",v.Local.sparams)
		F.Intrinsic.String.Split(v.Local.sparams,"*!*",v.Local.sparams)
		f.Intrinsic.String.Concat(V.Local.sJob,"*!*",V.Local.sWC,V.Local.sVals)
		f.Intrinsic.String.Split(v.Local.sVals,"*!*",v.Local.svals)
		'Print Report
		f.global.BI.PrintReport(v.Local.lBIID,0,v.Local.sparams,v.Local.sVals,"",false)
	
f.Intrinsic.Control.EndSelect
Program.Sub.gsgcFlexcover_RowCellClick.End

