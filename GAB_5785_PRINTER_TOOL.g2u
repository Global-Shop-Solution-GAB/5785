Program.Sub.gsgcSingle_RowCellClick.Start
v.Local.sPart.Declare
v.Local.sparams.Declare
v.Local.iRefID.Declare
v.Local.sLinkID.Declare
v.Local.iLinkID.Declare
V.Local.sTemp.Declare
v.Local.iPID.Declare
v.Local.sSQL.Declare
v.Local.i.Declare

F.Intrinsic.Control.SelectCase(v.Args.Column)
	F.Intrinsic.Control.CaseAny("PART","Part")
		v.Local.sPart.Set(v.Args.cellvalue)
		F.Intrinsic.String.Build("{0}!*!{1}!*!M",v.Local.sPart,"",V.Local.sParams)
		F.Global.General.CallWrapperSync(300011,V.Local.sParams)
		gui.frmPrint..SetFocus
		
	f.Intrinsic.Control.caseany("DOCUMENTS","Documents")
		Gui.frmPrint.gsgcSingle.GetCellValueByColumnName("gvSing","Part",V.Args.RowIndex,V.Local.sPart)
		f.Global.DocumentControl.GetLinkID(v.Local.sPart.Trim,30,v.Local.iRefID)
		f.Intrinsic.Control.If(v.Local.irefid,=,-1)
			F.ODBC.Connection!con.ExecuteAndReturn("SELECT MAX(LINK_ID) AS 'LinkID' FROM LINK_DATA",v.Local.sLinkID)
			f.Intrinsic.Math.ConvertToLong(v.Local.sLinkID,v.Local.iLinkID)
			f.Intrinsic.Math.Add(v.Local.iLinkID,1,v.Local.iLinkID)
			f.Intrinsic.string.LPad(v.Local.iLinkID,"0",9,v.Local.sLinkID)
			V.Local.iRefID.Set(v.Local.iLinkID)
			F.Global.DocumentControl.CreateReference(V.Local.sPart,"00030",V.Local.iRefID)
		f.Intrinsic.Control.EndIf
		F.Global.DocumentControl.Invoke(V.Local.sPart,30,"(Inventory Master)",V.Local.iPID)
		
	f.Intrinsic.Control.caseany("PRINTDOCS","PrintDocs")
		Gui.frmPrint.gsgcSingle.GetCellValueByColumnName("gvSing","Part",V.Args.RowIndex,V.Local.sPart)
		f.Global.DocumentControl.GetLinkID(v.Local.sPart.Trim,30,v.Local.iLinkID)
		f.Intrinsic.Control.If(v.Local.irefid,<>,-1)
			F.Intrinsic.String.Concat("SELECT * FROM ATG_DOC_ASSOC WHERE LINK_ID = ",V.Local.iLinkID," AND GRP_ID = 3",V.Local.sSQL)
			f.Data.DataTable.CreateFromSQL("dtDocs","con",v.Local.ssql)
			F.Intrinsic.Control.If(V.DataTable.dtDocs.RowCount,=,0)
				F.Intrinsic.UI.Msgbox("No Shop Documents to Print.")
			f.Intrinsic.Control.Else
				f.Intrinsic.Control.For(v.Local.i,0,v.DataTable.dtDocs.RowCount--,1)
					F.Intrinsic.String.Right(v.DataTable.dtDocs(v.Local.i).PATH!fieldvaltrim,1,V.Local.stemp)
					F.Intrinsic.Control.If(V.Local.stemp,=,"\")
						F.Intrinsic.String.Concat(v.DataTable.dtDocs(v.Local.i).PATH!fieldvaltrim,v.DataTable.dtdocs(v.Local.i).FILE!fieldvaltrim,V.Local.stemp)
					F.Intrinsic.Control.Else
						F.Intrinsic.String.Concat(v.DataTable.dtDocs(v.Local.i).PATH!FIELDVALTRIM,"\",v.DataTable.dtdocs(v.Local.i).FILE!fieldvaltrim,V.Local.stemp)
					F.Intrinsic.Control.endif												
					F.Intrinsic.task.ShellExec(V.Caller.Handle,"PRINT",V.local.sTemp,"","",5)
					f.Intrinsic.UI.SleepMS(3000)
				f.Intrinsic.Control.Next(v.Local.i)
			F.Intrinsic.Control.EndIf
		f.Intrinsic.Control.EndIf
		
f.Intrinsic.Control.EndSelect

Program.Sub.gsgcSingle_RowCellClick.End

Program.Sub.gsgcMulti_RowCellClick.Start
