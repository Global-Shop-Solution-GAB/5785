Program.Sub.GrabWO.Start
F.Intrinsic.Control.Try
	v.Local.ssql.Declare
	v.Local.sSO.Declare
	V.Local.sEmpNo.Declare
	v.Local.sParam.Declare
	v.Local.sJob.Declare
	v.Local.sSuffix.Declare
	v.Local.sSeq.Declare
	v.Local.sWC.Declare
	v.Local.sMsg.Declare
	v.Local.i.Declare
	v.Local.j.Declare(long)
	V.Local.k.Declare(long)
	v.Local.sSO.Set(v.Screen.frmPrint!txtSONumb.text)
	v.Local.sret.Declare
	'Hide grids
	gui.frmPrint.gsgcFlexSched.visible(false)
	gui.frmPrint.gsgcMulti.visible(false)
	gui.frmPrint.gsgcSingle.visible(false)
	gui.frmPrint.gsgcFlexcover.visible(false)
	
	'Get employee
	F.Global.Security.GetEmpNoFromUser(v.Caller.User,v.Local.sEmpNo)
	
	'Grab Single Level drawings
	f.Intrinsic.String.Build("SELECT SALES_ORDER, SALES_ORDER_LINE, rtrim(PART) as Part,  rtrim(DESCRIPTION) As 'Description' , CAST(0 AS BIT) AS 'SelectDocLine', 'Print' as 'PrintDocs', 'DCS-OFF' as 'Documents' FROM V_JOB_HEADER WHERE SALES_ORDER='{0}'",v.Local.sSO.Trim,v.Local.ssql)
	f.Intrinsic.Control.If(v.DataTable.dtSing.Exists)
		f.Data.DataTable.Close("dtSing")
	f.Intrinsic.Control.EndIf
	f.Data.DataTable.CreateFromSQL("dtSing","con",v.Local.ssql,true)
		
	'Grab BOM drawings
	f.Intrinsic.String.Build("SELECT SALES_ORDER, SALES_ORDER_LINE, rtrim(A.PART) as Part,  rtrim(DESCRIPTION) As 'Description', CAST(0 AS BIT) AS 'SelectLine', 1 AS 'Copies', 'Print Copies' as 'PrintCopies',  CAST(0 AS BIT) AS 'SelectDocLine', 'Print' as 'PrintDocs', 'DCS-OFF' as 'Documents' FROM V_JOB_HEADER A INNER JOIN V_BOM_MSTR B ON A.PART=B.PARENT WHERE SALES_ORDER='{0}'",v.Local.sSO.Trim,v.Local.ssql)
	f.Intrinsic.Control.If(v.DataTable.dtBOMTMP.Exists)
		f.Data.DataTable.Close("dtBOMTMP")
		F.Data.DataTable.Close("dtBOM")
	f.Intrinsic.Control.EndIf
	f.Data.DataTable.CreateFromSQL("dtBOMTMP","con",v.Local.ssql,true)
	f.Data.DataView.Create("dtBOMTMP","dvBOMTMP")
	F.Data.DataView.ToDataTableDistinct("dtBOMTMP","dvBOMTMP","dtBOM","SALES_ORDER*!*SALES_ORDER_LINE*!*PART*!*DESCRIPTION*!*SelectLine*!*COPIES*!*PRINTCOPIES*!*SelectDocLine*!*PRINTDOCS*!*DOCUMENTS",true)
	gui.frmPrint.gsgcMulti.DataSource("dtBOM")
	
	'Create Flex schedule report table basis
	f.Intrinsic.Control.If(v.DataTable.dtFlex.Exists)
		f.Data.DataTable.Close("dtFlex")
	f.Intrinsic.Control.EndIf
	F.Data.DataTable.CreateFromSQL("dtFlex","con","SELECT  SCHEDULE, LABOR_WC, '' as OPERATION,  CAST(0 AS BIT) AS 'SelectLine', 'Print Flex' as 'PrintFlex', CAST(0 AS BIT) AS 'SelectDocLine', 'Print Docs' as 'PrintDocs' FROM V_JOB_CT_FLX_SCHD WHERE JOB='BOGUSJ123'",true)
	f.Data.DataView.Create("dtFlex","dvFlex")
	
	'Populate the datatables document link from dictionary
	F.Data.Dictionary.CreateFromSQL("dDCC","con","Select rtrim(ID) As Part,'DCS-ON' As DCC From Link_Data Where Type = '00030'")
	F.Data.Dictionary.SetDefaultReturn("dDCC","DCS-OFF")
	F.Data.DataTable.FillFromDictionary("dtSing","dDCC","Part","Documents")
	F.Data.DataTable.FillFromDictionary("dtBOM","dDCC","Part","Documents")
	F.Data.Dictionary.Close("dDCC")
	
	'Delete from dtSing where bom exists
	F.Intrinsic.Control.For(v.local.k,v.DataTable.dtSing.RowCount--,0,-1)
		f.Intrinsic.String.Build("SELECT * FROM V_BOM_MSTR WHERE PARENT='{0}'",v.DataTable.dtSing(v.Local.k).PART!FIELDVALTRIM,v.Local.ssql)
		f.ODBC.Connection!con.executeandreturn(v.Local.ssql,v.Local.sret)
		f.Intrinsic.Control.If(v.Local.sret.Trim,<>,"")
			f.Data.DataTable.DeleteRow("dtSing",v.Local.k)
		f.Intrinsic.Control.EndIf
	F.Intrinsic.Control.Next(v.Local.k)
	gui.frmPrint.gsgcSingle.DataSource("dtSing")
	
	'Scroll through the jobs
	f.Intrinsic.string.Build("SELECT * FROM V_JOB_HEADER WHERE SALES_ORDER='{0}'",v.Local.sSO.Trim,v.Local.ssql)
	f.Intrinsic.Control.If(v.DataTable.dtJobs.exists)
		f.Data.DataTable.Close("dtJobs")
	f.Intrinsic.Control.EndIf
	f.Data.DataTable.CreateFromSQL("dtJobs","con",v.Local.ssql,true)
	f.Intrinsic.Control.For(v.Local.i,0,v.DataTable.dtJobs.RowCount--,1)
		'Set the params
		v.Local.sJob.Set(v.DataTable.dtJobs(v.Local.i).JOB!FIELDVALTRIM)
		V.Local.sSuffix.Set(V.DataTable.dtJobs(V.Local.I).SUFFIX!FIELDVALTRIM)

		'Grab Flex Schedules
		f.Intrinsic.String.Build("SELECT DISTINCT  CAST(0 AS BIT) AS 'SelectLine', CAST(0 AS BIT) AS 'SelectDocLine', SCHEDULE, LABOR_WC, OPERATION, 'Print Flex' as 'PrintFlex', 'Print Docs' as 'PrintDocs'  FROM V_JOB_CT_FLX_SCHD A LEFT JOIN V_JOB_OPERATIONS B ON A.JOB = B.JOB AND A.SUFFIX = B.SUFFIX AND A.LABOR_SEQ = B.SEQ WHERE A.JOB='{0}' AND A.SUFFIX='{1}'",v.Local.sJob,v.Local.sSuffix,v.Local.ssql)
		f.Intrinsic.Control.If(v.DataTable.dtFlexTmp.Exists)
			f.Data.DataTable.Close("dtFlexTmp")
		f.Intrinsic.Control.EndIf
		f.Data.DataTable.CreateFromSQL("dtFlexTmp","con",v.Local.ssql)
		F.Data.DataTable.Merge("dtFlexTmp","dtFlex",true,1)
		
		f.Intrinsic.Control.If(v.Args.Logon,=,"Y")
			'Design Option
			f.Intrinsic.Control.If(v.Screen.frmPrint!optDes.Value,=,"True")
				f.Intrinsic.String.Build("SELECT SEQ, PART FROM V_JOB_OPERATIONS WHERE JOB='{0}' AND SUFFIX='{1}' AND (PART='DWPD' or PART='DWRD')",v.DataTable.dtJobs(v.Local.i).JOB!FIELDVALTRIM,V.DataTable.dtJobs(v.Local.i).SUFFIX!FIELDVALTRIM,v.Local.ssql)
				f.Intrinsic.Control.If(v.DataTable.dtTmpSeq.Exists)
					f.Data.DataTable.Close("dtTmpSeq")
				f.Intrinsic.Control.EndIf
				f.Data.DataTable.CreateFromSQL("dtTmpSeq","con",v.Local.ssql)
				f.Intrinsic.Control.For(v.Local.j,0,v.DataTable.dtTmpSeq.RowCount--,1)
					v.Local.sSeq.Set(v.DataTable.dtTmpSeq(v.Local.j).SEQ!FIELDVALTRIM)
					v.Local.sWC.Set(v.DataTable.dtTmpSeq(v.Local.j).PART!FIELDVALTRIM)
					'Log employee onto job
					f.Intrinsic.String.ConcatCallWrapperArgs(v.Local.sjob,V.Local.sSuffix,V.Local.sSeq,V.Local.sWC,v.local.sEmpNo,v.Local.sParam)
					f.Global.General.CallWrapperAsync(9200,v.Local.sParam)
					f.Intrinsic.String.Concat(v.Local.sMsg,"Job ",v.Local.sJob,"-",v.Local.sSuffix," logged on",v.Ambient.NewLine,v.Local.sMsg)
				f.Intrinsic.Control.Next(v.Local.j)
			f.Intrinsic.Control.EndIf
			
			'Check option
			f.Intrinsic.Control.If(v.Screen.frmPrint!optChk.Value,=,"True")
				f.Intrinsic.String.Build("SELECT SEQ, PART FROM V_JOB_OPERATIONS WHERE JOB='{0}' AND SUFFIX='{1}' AND (PART='DWPC' or PART='DWRC')",v.DataTable.dtJobs(v.Local.i).JOB!FIELDVALTRIM,V.DataTable.dtJobs(v.Local.i).SUFFIX!FIELDVALTRIM,v.Local.ssql)
				f.Intrinsic.Control.If(v.DataTable.dtTmpSeq.Exists)
					f.Data.DataTable.Close("dtTmpSeq")
				f.Intrinsic.Control.EndIf
				f.Data.DataTable.CreateFromSQL("dtTmpSeq","con",v.Local.ssql)
				f.Intrinsic.Control.For(v.Local.j,0,v.DataTable.dtTmpSeq.RowCount--,1)
					v.Local.sSeq.Set(v.DataTable.dtTmpSeq(v.Local.j).SEQ!FIELDVALTRIM)
					v.Local.sWC.Set(v.DataTable.dtTmpSeq(v.Local.j).PART!FIELDVALTRIM)
					'Log employee onto job
					f.Intrinsic.String.ConcatCallWrapperArgs(v.Local.sjob,V.Local.sSuffix,V.Local.sSeq,V.Local.sWC,v.local.sEmpNo,v.Local.sParam)
					f.Global.General.CallWrapperAsync(9200,v.Local.sParam)
					f.Intrinsic.String.Concat(v.Local.sMsg,"Job ",v.Local.sJob,"-",v.Local.sSuffix," logged on",v.Ambient.NewLine,v.Local.sMsg)
				f.Intrinsic.Control.Next(v.Local.j)
			f.Intrinsic.Control.EndIf
	
		f.Intrinsic.Control.EndIf	
	f.Intrinsic.Control.Next(v.Local.i)
	
	'Set to 0
	V.Local.k.Set(0)
	v.Local.j.Set(0)
	
	f.Intrinsic.Control.If(v.Args.Logon,=,"Y")
		'Display Message
		f.Intrinsic.Control.If(v.Local.sMsg.Trim,=,"")
			f.Intrinsic.UI.Msgbox("No jobs to log onto")
		f.Intrinsic.Control.Else
			f.Intrinsic.UI.Msgbox(v.Local.sMsg)
		f.Intrinsic.Control.EndIf
	f.Intrinsic.Control.EndIf
	
	'All Distinct Flex Schdules
	f.Intrinsic.Control.If(v.DataTable.dtFlexDist.Exists)
		f.Data.DataTable.Close("dtFlexDist")
	f.Intrinsic.Control.EndIf
	f.Data.DataView.ToDataTableDistinct("dtFlex","dvFlex","dtFlexDist","SCHEDULE*!*LABOR_WC*!*OPERATION*!*SELECTLINE*!*PRINTFLEX*!*SELECTDOCLINE*!*PRINTDOCS",true)
	gui.frmPrint.gsgcFlexSched.DataSource("dtFlexDist")
	
	'Flex WC Distinct
	f.Intrinsic.Control.If(v.DataTable.dtFlexWCDist.Exists)
		f.Data.DataTable.Close("dtFlexWCDist")
	f.Intrinsic.Control.EndIf
	f.Data.DataView.ToDataTableDistinct("dtFlex","dvFlex","dtFlexWCDist","LABOR_WC",true)
	
	'Create flex Cover datatable
	f.Intrinsic.Control.If(v.DataTable.dtFlexCovr.Exists)
		f.Data.DataTable.Close("dtFlexCovr")
	f.Intrinsic.Control.EndIf
	f.Data.DataTable.Create("dtFlexCovr",true)
	f.Data.DataTable.AddColumn("dtFlexCovr","Job","string")
	f.Data.DataTable.AddColumn("dtFlexCovr","WC","string")
	f.Data.DataTable.AddColumn("dtFlexCovr","SelectLine","boolean")
	f.Data.DataTable.AddColumn("dtFlexCovr","PrintCover","string","Print Cover")
	
	'Create distinct job master table
	f.Intrinsic.Control.If(v.DataTable.dtjobsmast.Exists)
		f.Data.DataTable.Close("dtJobsMast")
	f.Intrinsic.Control.EndIf
	f.Data.DataView.Create("dtJobs","dvJobs")
	f.Data.DataView.ToDataTableDistinct("dtJobs","dvJobs","dtJobsMast","JOB",true)
	
	'Add the jobs to the flex cover database
	f.Intrinsic.Control.For(v.Local.k,0,v.DataTable.dtFlexWCDist.RowCount--,1)
		f.Intrinsic.Control.For(v.Local.j,0,v.DataTable.dtjobsmast.RowCount--,1)
			F.Data.DataTable.AddRow("dtFlexCovr","Job",v.DataTable.dtJobsmast(v.Local.j).JOB!FIELDVALTRIM,"WC",v.DataTable.dtflexWCDist(v.Local.k).LABOR_WC!FIELDVALTRIM,"SelectLine",False)
		f.Intrinsic.Control.Next(v.Local.j)
	f.Intrinsic.Control.Next(v.Local.k)
	
	gui.frmPrint.gsgcFlexcover.DataSource("dtFlexCovr")
	
	f.Intrinsic.Control.CallSub(SetColumnProperties)
	
	'Show Grids
	gui.frmPrint.gsgcFlexcover.visible(true)
	gui.frmPrint.gsgcFlexSched.visible(true)
	gui.frmPrint.gsgcMulti.visible(true)
	gui.frmPrint.gsgcSingle.visible(true)

F.Intrinsic.Control.Catch
	f.Intrinsic.UI.Msgbox(v.Ambient.ErrorDescription)
	F.Intrinsic.Control.CallSub(frmPrint_Unload)
F.Intrinsic.Control.EndTry
Program.Sub.GrabWO.End

Program.Sub.cmdBrowse_Click.Start
V.Local.sSQL.Declare(String)
V.Local.sCol.Declare(String)
V.Local.sColW.Declare(String)
V.Local.sRet.Declare(String)
V.Local.sSQL.set("SELECT ORDER_NO, NAME_CUSTOMER FROM V_ORDER_BILL_TO")
'Split Column Names into an array (*!* delimited string)
F.Intrinsic.String.Split("Order#*!*Customer Name","*!*",V.Local.sCol)
'Split Column widths into an array (*!* delimited string)
F.Intrinsic.String.Split("725*!*3100","*!*",V.Local.sColW)
F.Intrinsic.UI.Browser("Select an Order","con",V.Local.sSQL,V.Local.sCol,V.Local.sColW,V.Local.sRet)
F.Intrinsic.Control.If(V.Local.sRet,<>,"***CANCEL***")
	f.Intrinsic.String.Split(v.Local.sRet,"*!*",v.Local.sRet)
	gui.frmPrint.txtSONumb.Text(v.Local.sRet(0))
	f.Intrinsic.Control.CallSub(GrabWO,"Logon","N")
F.Intrinsic.Control.EndIf
Program.Sub.cmdBrowse_Click.End

